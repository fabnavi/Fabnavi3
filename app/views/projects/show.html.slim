-content_for( :js_include ) do
  =javascript_include_tag 'lib/jquery',  'lib/jquery-ui/jquery-ui.js',  'ImageConverter',
    'ImageList',  'Detail',  'ViewConfig',  'ThumbnailViewer',  'Key',
    'Publisher',
    'CameraControll',  'Server',  'CalibrateController',  'ImageUploadQueue',
    'PhaseController',  'Fabnavi',  'MainView'
  javascript:
    function boot(){
      Fabnavi.play();
    }
-content_for( :css_link ) do
  =stylesheet_link_tag 'content', media: 'all', 'data-turbolinks-track' => true 
=render partial: "layouts/content", locals: { project: @project.to_json, pictures: @project.try(:picture)}
